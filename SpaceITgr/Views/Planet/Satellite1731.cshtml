@page
@using SpaceITgr.Models;
@model SpaceITgr.Views.Planet.Satellite171Model
@{
	ViewBag.CodeName = $"{SpaceData.Planets[1731].CodeName}";
	ViewBag.CurrentTime = SpaceData.Planets[1731].CurrentTime.ToString("o");
	ViewBag.LocalName = $"{SpaceData.Planets[1731].LocalName}";
	ViewBag.URL = $"/Images/Moon.png";
	ViewBag.PlanetColor = "black";
	ViewBag.CityRoute = SpaceData.Locations["c3"].Route;
	ViewBag.VillageRoute = SpaceData.Locations["v3"].Route;
	ViewBag.SurfaceRoute = SpaceData.Locations["s3"].Route;
	ViewBag.CityTransform = new Dictionary<char, int>()
	{
			{'x', 1050},
			{'y', 1000},
			{'r', 9}
	};
	ViewBag.VillageTransform = new Dictionary<char, int>()
	{
			{'x', 1200},
			{'y', 900},
			{'r', 8}
	};
	ViewBag.SurfceTransform = new Dictionary<char, string>()
	{
			{'x', 1400.ToString()},
			{'y', 1200.ToString()},
			{'r', 7.ToString()},
			{'d', "Лунные гейзеры"}
	};
	Layout = "/Views/Shared/_Layout.cshtml";
}

